.cycle-slideshow {
		width: 45%;
		min-width: 1200px;
		max-width: 400px;
		margin:  auto;
		padding: 0;
		position: relative;
}
.cycle-slideshow img {
		position: absolute;
		top: 0;
		left: 0;
		width: 1200px;
		height: 300px;
		display: block;
		    margin-top: 22px;
}
.cycle-prev {
left: 0;
background: url(http://malsup.github.com/images/left.png) 50% 50% no-repeat;
}
.cycle-next {
right: 0;
background: url(http://malsup.github.com/images/right.png) 50% 50% no-repeat;
}
.cycle-next:hover {
opacity: .7;
filter: alpha(opacity=70);
}
.cycle-prev:hover {
opacity: .7;
filter: alpha(opacity=70);
}
.cycle-prev, .cycle-next {
position: absolute;
top: 0;
width: 8%;
opacity: 0;
filter: alpha(opacity=0);
z-index: 800;
height: 100%;
cursor: pointer;
}

